	<CheckButton name="AutoBar_Config_Button_Template" inherits="ActionButtonTemplate" virtual="true">
		<Size>
			<AbsDimension x="36" y="36"/>
		</Size>
		<Scripts>
			<OnLoad>
				this:RegisterForClicks("LeftButtonUp", "RightButtonUp");
			</OnLoad>
			<OnClick>
				this:SetChecked(0);
				AutoBar_Config_ButtonOnClick(arg1)
